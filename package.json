{
  "name": "litework",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000 --turbopack",
    "dev:stable": "./scripts/dev/dev-server-stable.sh",
    "dev:restart": "./scripts/dev/dev-restart.sh",
    "dev:kill": "pkill -f 'next dev' || lsof -ti:3000 | xargs kill -9 2>/dev/null || true",
    "dev:clean": "rm -rf .next && npm run dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "deploy": "./deploy.sh",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "analyze": "ANALYZE=true next build --webpack"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.77.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "critters": "^0.0.23",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.548.0",
    "next": "16.0.1",
    "next-pwa": "^5.6.0",
    "react": "19.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "19.2.0",
    "react-window": "^2.2.2",
    "recharts": "^3.3.0",
    "resend": "^6.4.0",
    "swr": "^2.3.6",
    "web-push": "^3.6.7",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-window": "^1.8.8",
    "@types/web-push": "^3.6.4",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "sharp": "^0.34.4",
    "tailwindcss": "^4",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^5"
  }
}
